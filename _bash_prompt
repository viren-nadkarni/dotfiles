# bash prompt
if [[ ! -z $BASH ]]; then
    if [[ $EUID -eq 0 ]]; then
        # root
        PS1="\[\033[01;33m\]\u@\h\[\033[m\] \[\033[01;31m\]\w\[\033[m\]\$git_branch\$git_dirty # "
    else
        PS1="\[\033[01;32m\]\u@\h\[\033[m\] \[\033[01;34m\]\w\[\033[m\]\$git_branch\$git_dirty \$ "
    fi
fi

